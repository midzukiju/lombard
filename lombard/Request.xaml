<Window x:Class="lombard.Request"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:lombard"
        mc:Ignorable="d"
        Title="Request" Height="450" Width="800">
    <Grid Background="#FFF7F2F0">
        <Image Margin="78,64,78,253" Source="/Group 24.png" Stretch="Fill"/>
        <Rectangle HorizontalAlignment="Center" Height="44" VerticalAlignment="Top" Width="800" Fill="#FF707B6D"/>
        <Label Content="Услуги" HorizontalAlignment="Left" Margin="83,190,0,0" VerticalAlignment="Top" FontSize="20" FontFamily="Bahnschrift" Foreground="#FF262D24"/>
        <Button Content="&#xA;" HorizontalAlignment="Left" Margin="252,224,0,0" VerticalAlignment="Top" Height="116" Width="116"  Foreground="#FFF7F2F0" BorderBrush="#FFF7F2F0" RenderTransformOrigin="0.828,0.5">
            <Button.Background>
                <ImageBrush ImageSource="/Group 26.png"/>
            </Button.Background>
        </Button>
        <Button Content="" HorizontalAlignment="Left" Height="116" Margin="435,224,0,0" VerticalAlignment="Top" Width="116"  BorderBrush="#FFF7F2F0" Foreground="#FFF7F2F0">
            <Button.Background>
                <ImageBrush ImageSource="/Group 23.png"/>
            </Button.Background>
        </Button>
        <Button Content="" HorizontalAlignment="Left" Height="116" Margin="609,224,0,0" VerticalAlignment="Top" Width="116" BorderBrush="#FFF7F2F0" Foreground="#FFF7F2F0">
            <Button.Background>
                <ImageBrush ImageSource="/Group 27.png"/>
            </Button.Background>
        </Button>
        <Label Content="Каталог" HorizontalAlignment="Left" Margin="75,341,0,0" VerticalAlignment="Top" FontSize="20" FontFamily="Bahnschrift" Foreground="#FF262D24"/>
        <Button Content="" HorizontalAlignment="Left" Height="67" Margin="75,369,0,0" VerticalAlignment="Top" Width="188" Foreground="#FFF7F2F0" BorderBrush="#FFF7F2F0" >
            <Button.Background>
                <ImageBrush ImageSource="/Group 28.png"/>
            </Button.Background>
        </Button>
        <Button Content="&#xA;" HorizontalAlignment="Center" Height="67" Margin="0,369,0,0" VerticalAlignment="Top" Width="188" BorderBrush="#FFF7F2F0" Foreground="#FFF7F2F0">
            <Button.Background>
                <ImageBrush ImageSource="/Group 29.png"/>
            </Button.Background>
        </Button>
        <Button Content="" HorizontalAlignment="Left" Height="67" Margin="537,369,0,0" VerticalAlignment="Top" Width="188" BorderBrush="#FFF7F2F0" Foreground="#FFF7F2F0">
            <Button.Background>
                <ImageBrush ImageSource="/Group 30.png"/>
            </Button.Background>
        </Button>
        <Label Content="Бриллиантовый дом" HorizontalAlignment="Left" Margin="94,2,0,0" VerticalAlignment="Top" FontSize="26" FontFamily="Bahnschrift" Foreground="#FFF7F2F0"/>
        <Button Content="" HorizontalAlignment="Left" Height="30" Margin="730,6,0,0" VerticalAlignment="Top" Width="29" BorderBrush="#FF707B6D" Foreground="#FF707B6D">
            <Button.Background>
                <ImageBrush ImageSource="/gg_profile.png"/>
            </Button.Background>
        </Button>
        <ComboBox HorizontalAlignment="Left" Margin="518,6,0,0" VerticalAlignment="Top" Width="181" Height="30" />
        <Button Content="" HorizontalAlignment="Left" Height="116" Margin="78,224,0,0" VerticalAlignment="Top" Width="116" BorderBrush="#FFF7F2F0" Foreground="#FFF7F2F0">
            <Button.Background>
                <ImageBrush ImageSource="/Group 23.png"/>
            </Button.Background>
        </Button>
        <Button Content="" HorizontalAlignment="Left" Height="28" Margin="34,8,0,0" VerticalAlignment="Top" Width="29" BorderBrush="#FF707B6D" Foreground="#FF707B6D">
            <Button.Background>
                <ImageBrush ImageSource="/meteor-icons_list.png"/>
            </Button.Background>
        </Button>
        <Rectangle HorizontalAlignment="Center" Height="436" Stroke="Black" VerticalAlignment="Center" Width="800">
            <Rectangle.Fill>
                <SolidColorBrush Color="#FFD9D9D9" Opacity="0.68"/>
            </Rectangle.Fill>
        </Rectangle>
        <Rectangle HorizontalAlignment="Center" Height="379" Stroke="#FF707B6D" VerticalAlignment="Top" Width="412" Fill="#FFD9D9D9" Margin="0,23,0,0"/>
        <Label Content="Заявка" HorizontalAlignment="Center" Margin="0,32,0,0" VerticalAlignment="Top" FontFamily="Bahnschrift" FontSize="18" Foreground="#FF262D24"/>
        <Button Content="" HorizontalAlignment="Left" Height="31" Margin="563,32,0,0" VerticalAlignment="Top" Width="30" BorderBrush="#FFD9D9D9" RenderTransformOrigin="0.535,1.24" Click="Button_Click_1">
            <Button.Background>
                <ImageBrush ImageSource="/hugeicons_cancel-circle.png"/>
            </Button.Background>
        </Button>
        <TextBox x:Name="PhoneTextBox" HorizontalAlignment="Center" Height="28" Margin="0,246,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="356" FontFamily="Bahnschrift">
            <TextBox.Foreground>
                <SolidColorBrush Color="#FF262D24" Opacity="1"/>
            </TextBox.Foreground>
        </TextBox>
        <TextBox x:Name="CityTextBox" HorizontalAlignment="Center" Height="28" Margin="0,300,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="356" TextChanged="TextBox_TextChanged" FontFamily="Bahnschrift">
            <TextBox.Foreground>
                <SolidColorBrush Color="#FF262D24" Opacity="1"/>
            </TextBox.Foreground>
        </TextBox>
        <TextBox x:Name="PTextBox" HorizontalAlignment="Center" Height="28" Margin="0,192,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="356" FontFamily="Bahnschrift">
            <TextBox.Foreground>
                <SolidColorBrush Color="#FF262D24" Opacity="1"/>
            </TextBox.Foreground>
        </TextBox>
        <Button Content="Оставить заявку" HorizontalAlignment="Center" Height="35" Margin="0,338,0,0" VerticalAlignment="Top" Width="158" Click="Button_Click" Background="#FF707B6D" BorderBrush="#FF707B6D" FontFamily="Bahnschrift" FontSize="14" Foreground="#FFF7F2F0"/>
        <Label Content="Введите Отчество" HorizontalAlignment="Left" Margin="220,168,0,0" VerticalAlignment="Top" FontFamily="Bahnschrift">
            <Label.Foreground>
                <SolidColorBrush Color="#FF262D24" Opacity="0.5"/>
            </Label.Foreground>
        </Label>
        <Label Content="Введите номер телефона" HorizontalAlignment="Left" Margin="217,224,0,0" VerticalAlignment="Top" FontFamily="Bahnschrift">
            <Label.Foreground>
                <SolidColorBrush Color="#FF262D24" Opacity="0.5"/>
            </Label.Foreground>
        </Label>
        <Label Content="Введите город" HorizontalAlignment="Left" Margin="217,278,0,0" VerticalAlignment="Top" FontFamily="Bahnschrift">
            <Label.Foreground>
                <SolidColorBrush Color="#FF262D24" Opacity="0.5"/>
            </Label.Foreground>
        </Label>
        <Label Content="Введите Фамилию" HorizontalAlignment="Left" Margin="218,61,0,0" VerticalAlignment="Top" FontFamily="Bahnschrift">
            <Label.Foreground>
                <SolidColorBrush Color="#FF262D24" Opacity="0.5"/>
            </Label.Foreground>
        </Label>
        <TextBox x:Name="FTextBox" HorizontalAlignment="Center" Height="28" Margin="0,83,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="356" FontFamily="Bahnschrift">
            <TextBox.Foreground>
                <SolidColorBrush Color="#FF262D24" Opacity="1"/>
            </TextBox.Foreground>
        </TextBox>
        <Label Content="Введите Имя" HorizontalAlignment="Left" Margin="220,114,0,0" VerticalAlignment="Top" FontFamily="Bahnschrift">
            <Label.Foreground>
                <SolidColorBrush Color="#FF262D24" Opacity="0.5"/>
            </Label.Foreground>
        </Label>
        <TextBox x:Name="ITextBox" HorizontalAlignment="Center" Height="28" Margin="0,137,0,0" TextWrapping="Wrap" Text="" VerticalAlignment="Top" Width="356" FontFamily="Bahnschrift">
            <TextBox.Foreground>
                <SolidColorBrush Color="#FF262D24" Opacity="1"/>
            </TextBox.Foreground>
        </TextBox>

    </Grid>
</Window>
